{
  "hosting": {
    "public": "dist/Angular",
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    } ],


    "headers": [ {

      "source": "**/*.@(jpg|psd)",
      "headers": [ {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      } ]
    }]
  }
}


